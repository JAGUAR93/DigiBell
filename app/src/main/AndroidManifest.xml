<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.digi_bell">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Digi_Bell">


        <activity android:name=".Login" />
        <activity android:name=".NumberUpdate" />
        <activity android:name=".GestureMain" />
        <activity android:name=".Generator1" />
        <activity android:name=".ScanReceive" />
        <activity android:name=".Receiver" />
        <activity android:name=".SignUp" />
        <activity android:name=".Contact" />
        <activity android:name=".Personal_info" />
        <activity android:name=".UserProfile" />
        <activity android:name=".HomeScreen2" />
        <activity android:name=".HomeScreen1" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>