<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ar.digi_bell">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appkafinalicon_foreground"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/appkafinalicon_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Digi_Bell.NoActionBar.">
        <activity android:name="com.ar.digi_bell.Scanner"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar."/>

        <activity
            android:name="com.ar.digi_bell.Login"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity
            android:name="com.ar.digi_bell.NumberUpdate"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity
            android:name="com.ar.digi_bell.GestureMain"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity
            android:name="com.ar.digi_bell.Generator1"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity
            android:name="com.ar.digi_bell.ScanReceive"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity
            android:name="com.ar.digi_bell.Receiver"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity
            android:name="com.ar.digi_bell.SignUp"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity
            android:name="com.ar.digi_bell.Personal_info"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity
            android:name="com.ar.digi_bell.HomeScreen2"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity
            android:name="com.ar.digi_bell.HomeScreen1"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Digi_Bell.NoActionBar." />
        <activity android:name="com.ar.digi_bell.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>